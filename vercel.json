{
  "version": 2,
  "redirects": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml",
      "statusCode": 200
    },
    {
      "source": "/404.html",
      "destination": "/404",
      "statusCode": 200
    },
    {
      "source": "/500.html",
      "destination": "/500",
      "statusCode": 200
    },
    {
      "source": "/:path*.:ext(php|htm|html|xhtml|xml)",
      "destination": "/:path*",
      "statusCode": 302
    },
    {
      "source": "/(.*)//(.*)",
      "destination": "/$1/$2",
      "statusCode": 302
    },
    {
      "source": "/(.*)/$",
      "destination": "/$1",
      "statusCode": 302
    },
    {
      "source": "/:page(index):ext(\\.(php|html|xhtml|htm|xml))?",
      "destination": "/",
      "statusCode": 302
    },
    {
      "source": "/:page(index|default|home|test):ext(\\.(php|html|xhtml|htm|xml))?",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.vercel-test-redirects-inky.vercel.app"
        }
      ],
      "destination": "https://vercel-test-redirects-inky.vercel.app/:path*",
      "permanent": false
    }
  ]
}
